

const fibo <- (n: int): int => {

if n = 0 then
	return 0;
else if n = 1 then
	return  1;
else
	return fibo(n-1)+fibo(n-2);
fi;
};

const start <- (): int => {
let n: int;	
n <- readInt();
writeString("Fibonacci number of ");
writeInt(n);
writeString(" is : ");
writeInt(fibo(n));
writeString("\n\n");
}
